      function LoginComponent(formBuilder, ngZone, router, commonUtility, loginService) {
        _classCallCheck(this, LoginComponent);

        this.formBuilder = formBuilder;
        this.ngZone = ngZone;
        this.router = router;
        this.commonUtility = commonUtility;
        this.loginService = loginService;
        this.loginForm = this.formBuilder.group({
          account: '',
          pwd: ''
        });
      }

      _createClass(LoginComponent, [{
        key: "onClickSubmit",
        value: function onClickSubmit(data) {
          var _this = this;

          // console.log('You are login',data);
          sessionStorage.clear();
          this.loginService.appLogin(data.account, data.pwd).subscribe(function (res) {
            console.log('login OK');
            console.log(res);

            switch (res.ResponseDetails.responseCode) {
              case "000":
                _this.commonUtility.TokenData = res.TokenData;
                _this.commonUtility.loginUser = data.account;

                _this.ngZone.run(function () {
                  return _this.router.navigateByUrl('/main');
                });

                break;

              case "008":
                break;

              default:
                break;
            }

            ;
          });
        }
